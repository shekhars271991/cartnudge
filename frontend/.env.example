# CartNudge Environment Variables
# Copy this file to .env and fill in your values

# API Configuration
VITE_API_URL=http://localhost:8001/api
VITE_WEBHOOK_BASE_URL=https://api.cartnudge.ai/v1/webhooks

# Demo API Keys (for display purposes only - not real keys)
VITE_DEMO_PUBLIC_KEY=cnk_pub_xxxxxxxxxxxxxxxxxxxx
VITE_DEMO_SECRET_KEY=cnk_sec_xxxxxxxxxxxxxxxxxxxx

# Third-party Integrations (optional)
VITE_GOOGLE_CLIENT_ID=
VITE_GITHUB_CLIENT_ID=

# Feature Flags
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_ANALYTICS=true
